<Defs>



<ThingDef ParentName="ArmorSmithableBase">
    <defName>Stalker_Apparel_ArmorMerc</defName>
    <label>Mercenary Suit</label>
    <description>Mercenary-designed suit, based on special forces gear. High molecular polyethylene plates and aramid padding not only sound futuristic, but provide excellent protection from high-corrosion Zone weapons.</description>
    <possessionCount>1</possessionCount>
  <recipeMaker Inherit="False">
     <researchPrerequisite>FlakArmor</researchPrerequisite>
      <recipeUsers Inherit="False">
				<li>TableMachining</li>
	  </recipeUsers>
      <workSkill>Crafting</workSkill>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
      <displayPriority>520</displayPriority>
    </recipeMaker>
	 <costList Inherit="False">
      <Plasteel>15</Plasteel>
      <ComponentSpacer>1</ComponentSpacer>
	  <Cloth>90</Cloth>
    </costList>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/Merc/Merc</texPath>
       <graphicClass>Graphic_Single</graphicClass> 
	  <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <WorkToMake>38000</WorkToMake>
      <MaxHitPoints>290</MaxHitPoints>
      <Mass>7</Mass>
      <StuffEffectMultiplierArmor>0.60</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.35</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.40</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>15</EquipDelay>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <equippedStatOffsets>
      <MoveSpeed>-0.8</MoveSpeed>
    </equippedStatOffsets>
    <thingCategories>
      <li>ApparelArmor</li>
    </thingCategories>
    <tradeTags>
      <li>Clothing</li>
      <li>Armor</li>
    </tradeTags>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/Merc/Merc</wornGraphicPath>
	   <useWornGraphicMask>true</useWornGraphicMask>
      <layers>
        <li>Middle</li>
        <li>Shell</li>
      </layers>
      <tags>
	    <li>LightArmor</li>
        <li>IndustrialMilitaryBasic</li>
      </tags>
      <defaultOutfitTags>
        <li>StalkerMerc</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0.33,0.33,0.33)</only>
